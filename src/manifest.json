{
	"name": "Wikimplify",
	"manifest_version": 2,
	"version": "0.1",
	"description": "",
	"browser_action":
	{
		"default_logo": "img/logo.png",
		"default_title": ""
	},
	"icons":
	{
		"48": "img/logo.png"
	},
	"background":
	{
		"scripts": ["background.js"]
	},
	"content_scripts":
	[
		{
			"matches": [
				"https://en.wikipedia.org/wiki/*",
				"http://en.wikipedia.org/wiki/*"
			],
			"js": ["content.js"]
		}
	],
	"permissions":
	[
		"activeTab"
	]
}